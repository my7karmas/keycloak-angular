{
  "name": "keycloak-angular",
  "version": "8.0.1",
  "description": "Easy Keycloak setup for Angular applications",
  "scripts": {
    "contributors:add": "node ./node_modules/all-contributors-cli/dist/cli add",
    "contributors:generate": "node ./node_modules/all-contributors-cli/dist/cli generate",
    "clean": "node ./node_modules/shx/lib/cli rm -rf ./dist/",
    "build": "npm run clean && node ./node_modules/@angular/cli/bin/ng build keycloak-angular --prod && node ./node_modules/shx/lib/cli cp ./README.md ./dist/keycloak-angular/",
    "test": "node ./node_modules/@angular/cli/bin/ng test keycloak-angular",
    "lint": "node ./node_modules/@angular/cli/bin/ng lint"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/mauriciovigolo/keycloak-angular.git"
  },
  "author": {
    "name": "Mauricio Gemelli Vigolo",
    "email": "@mauriciovigolo",
    "url": "https://github.com/mauriciovigolo"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mauriciovigolo/keycloak-angular/issues"
  },
  "homepage": "https://github.com/mauriciovigolo/keycloak-angular#readme",
  "keywords": [
    "angular",
    "authentication",
    "authorization",
    "keycloak",
    "keycloak-js"
  ],
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1000.2",
    "@angular-devkit/build-ng-packagr": "~0.1000.2",
    "@angular/cli": "^10.0.2",
    "@angular/common": "^10.0.3",
    "@angular/compiler": "^10.0.3",
    "@angular/compiler-cli": "^10.0.3",
    "@angular/core": "^10.0.3",
    "@angular/language-service": "^10.0.3",
    "@angular/platform-browser": "^10.0.3",
    "@angular/platform-browser-dynamic": "^10.0.3",
    "@angular/router": "^10.0.3",
    "@types/jasmine": "^3.5.11",
    "@types/jasminewd2": "~2.0.8",
    "@types/node": "^14.0.22",
    "all-contributors-cli": "^6.16.1",
    "keycloak-js": "https://github.com/my7karmas/keycloak",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.2",
    "karma": "~5.1.0",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~3.3.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "ng-packagr": "^10.0.1",
    "protractor": "~7.0.0",
    "puppeteer": "^5.2.1",
    "rxjs": "~6.6.0",
    "shx": "^0.3.2",
    "ts-node": "^8.10.2",
    "tslint": "~6.1.2",
    "typescript": "~3.9.6",
    "zone.js": "~0.10.3"
  },
  "dependencies": {
    "tslib": "^2.0.0"
  }
}
